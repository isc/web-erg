            <textevent timeoffset="20" message="This isn&apos;t a total recovery." locIndex="42" />
            <textevent timeoffset="30" message="Keep a little pressure on those pedals." locIndex="43" />
            <textevent timeoffset="40" message="Try a slowing down your cadence. Aim for 80-90 rpms." locIndex="44" />
            <textevent timeoffset="110" message="Next up is 4 minutes at zone 4, your &quot;threshold&quot;." locIndex="45" />
        </SteadyState>
        <SteadyState Duration="240" Power="1">
