            <textevent timeoffset="60" message="1 minute in, start to settle down a little bit. Find that natural rhythm again." locIndex="19" />
            <textevent timeoffset="180" message="Last minute here, finish this up strong!" locIndex="20" />
        </SteadyState>
        <SteadyState Duration="180" Power="0.50449997">
