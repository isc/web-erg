        </SteadyState>
        <IntervalsT Repeat="10" OnDuration="40" OffDuration="20" OnPower="1.0944999" OffPower="0.55449998" Cadence="95" CadenceResting="80">
            <textevent timeoffset="0" message="Okay, let&apos;s get into this! 40 seconds at a hard effort at 95 RPM!" locIndex="40" />
            <textevent timeoffset="30" message="10 more seconds than we can rest." locIndex="41" />
            <textevent timeoffset="40" message="Great work, now we take a short break for 20 seconds." locIndex="42" />
            <textevent timeoffset="60" message="And back up again, 40 more seconds here!" locIndex="43" />
            <textevent timeoffset="100" message="Excellent work bring it back down again for 20 seconds." locIndex="44" />
            <textevent timeoffset="140" message="Back up again!" locIndex="45" />
            <textevent timeoffset="300" message="Halfway there! You are looking great!" locIndex="46" />
            <textevent timeoffset="480" message="2 more to go! Stay strong here!" locIndex="47" />
            <textevent timeoffset="540" message="This is it, I want this as good as the 1st rep!" locIndex="48" />
            <textevent timeoffset="560" message="DIG DIG DIG!" locIndex="49" />
        </IntervalsT>
        <SteadyState Duration="300" Power="0.50449997">
            <textevent timeoffset="0" message="Great work again, that 2nd set always tickles more than the 1st!" locIndex="50" />
            <textevent timeoffset="60" message="To further build upon last week&apos;s efforts we are going to be finishing with a 3 minute interval at 125% FTP." locIndex="51" />
            <textevent timeoffset="70" message="This interval will really challenge you! But I know YOU can do this!" locIndex="52" />
