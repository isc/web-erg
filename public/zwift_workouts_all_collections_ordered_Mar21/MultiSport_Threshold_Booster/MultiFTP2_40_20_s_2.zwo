            <textevent timeoffset="30" message="10 more seconds than we can rest." locIndex="25" />
            <textevent timeoffset="40" message="Great work, now we take a short break for 20 seconds." locIndex="26" />
            <textevent timeoffset="60" message="And back up again, 40 more seconds here!" locIndex="27" />
            <textevent timeoffset="100" message="Excellent work bring it back down again for 20 seconds." locIndex="28" />
            <textevent timeoffset="140" message="Back up again!" locIndex="29" />
            <textevent timeoffset="300" message="Halfway there! You are looking great!" locIndex="30" />
            <textevent timeoffset="480" message="2 more to go! Stay strong here!" locIndex="31" />
            <textevent timeoffset="540" message="This is it, I want this as good as the 1st rep!" locIndex="32" />
            <textevent timeoffset="560" message="DIG DIG DIG!" locIndex="33" />
        </IntervalsT>
        <SteadyState Duration="300" Power="0.50449997">
