        </IntervalsT>
        <SteadyState Duration="290" Power="0.45" Cadence="80">
            <textevent timeoffset="260" message="Final block coming up." locIndex="34" />
            <textevent timeoffset="280" message="All seated accelerations this time." locIndex="35" />
        </SteadyState>
        <IntervalsT Repeat="8" OnDuration="20" OnPower="1.4" OffDuration="10" OffPower="0.45" Cadence="110" CadenceResting="90">
            <textevent timeoffset="0" message="Go!" locIndex="36" />
