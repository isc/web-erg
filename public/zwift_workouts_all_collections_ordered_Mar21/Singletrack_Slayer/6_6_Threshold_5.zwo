            <textevent timeoffset="360" message="Next sprint!" locIndex="48" />
            <textevent timeoffset="420" message="Do you feel you are recovering from the sprints at this threshold level? " locIndex="49" />
            <textevent timeoffset="480" message="Last sprint in this block!" locIndex="50" />
        </IntervalsT>
        <SteadyState Duration="180" Power="0.5">
            <textevent timeoffset="30" message="Great effort!" locIndex="51" />
            <textevent timeoffset="60" message="You have now completed 20min of threshold work!" locIndex="52" />
            <textevent timeoffset="100" message="A brief recovery here, then we do it again." locIndex="53" />
            <textevent timeoffset="0" message="One more set to go!" locIndex="54" />
        </SteadyState>
        <IntervalsT Repeat="5" OnDuration="15" OffDuration="105" OnPower="1.3" OffPower="0.9">
            <textevent timeoffset="0" message="Out of the seat and sprint!" locIndex="55" />
            <textevent timeoffset="30" message="Into a threshold effort now." locIndex="56" />
            <textevent timeoffset="100" message="The pressure is on the pedals." locIndex="57" />
            <textevent timeoffset="120" message="Sprint again!" locIndex="58" />
