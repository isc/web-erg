        <SteadyState Duration="300" Power="0.5">
            <textevent timeoffset="30" message="Awesome work!" locIndex="68" />
            <textevent timeoffset="50" message="30min of threshold work with 15 sprints along the way." locIndex="69" />
            <textevent timeoffset="70" message="That&apos;s a big day in any language!" locIndex="70" />
