            <textevent timeoffset="100" message="The power has also increased a little." locIndex="39" />
            <textevent timeoffset="160" message="Now you are flying along on the flat section of dirt." locIndex="40" />
            <textevent timeoffset="240" message="For the final step we up the power..." locIndex="41" />
            <textevent timeoffset="0" message="...but drop the cadence a little." locIndex="42" />
        </SteadyState>
        <SteadyState Duration="300" Power="0.91" Cadence="90">
            <textevent timeoffset="10" message="Straight to the target cadence of 90rpm." locIndex="43" />
            <textevent timeoffset="100" message="Today is all about getting zone 3 volume in." locIndex="44" />
            <textevent timeoffset="200" message="It doesn&apos;t have to be high intensity to be effective." locIndex="45" />
            <textevent timeoffset="240" message="You&apos;re doing great!" locIndex="46" />
        </SteadyState>
        <SteadyState Duration="180" Power="0.5">
            <textevent timeoffset="20" message="Fantastic!" locIndex="47" />
            <textevent timeoffset="80" message="That&apos;s 30min of tempo work so far." locIndex="48" />
            <textevent timeoffset="100" message="Now, for the sting in the tail." locIndex="49" />
            <textevent timeoffset="120" message="Next, we have a tempo effort but with no target cadence." locIndex="50" />
            <textevent timeoffset="140" message="Let&apos;s see what your natural self selected cadence is." locIndex="51" />
        </SteadyState>
        <SteadyState Duration="300" Power="0.85">
            <textevent timeoffset="30" message="Settle into a nice comfortable pedal stroke for you." locIndex="52" />
            <textevent timeoffset="60" message="What is your cadence?" locIndex="53" />
            <textevent timeoffset="80" message="High or low?" locIndex="54" />
            <textevent timeoffset="100" message="There really is no right or wrong." locIndex="55" />
            <textevent timeoffset="120" message="We find our best cadence that fits our fitness and fatigue." locIndex="56" />
        </SteadyState>
        <SteadyState Duration="300" Power="0.5">
            <textevent timeoffset="30" message="That&apos;s a nice 35min of tempo work today." locIndex="57" />
            <textevent timeoffset="60" message="You should be pleased with the work you completed today." locIndex="58" />
