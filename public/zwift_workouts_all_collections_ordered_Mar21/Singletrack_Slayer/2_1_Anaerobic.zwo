            <textevent timeoffset="210" message="Lactate is well above resting level now!" locIndex="43" />
            <textevent timeoffset="230" message="Ready for the 1st effort." locIndex="44" />
        </Ramp>
        <IntervalsT Repeat="4" OnDuration="60" OffDuration="120" OnPower="1.35" OffPower="0.5" pace="1481003871" Cadence="80" CadenceResting="90">
            <textevent timeoffset="10" message="Hit that target cadence of 80rpm." locIndex="45" />
            <textevent timeoffset="40" message="Keep the power up! You&apos;ve got this." locIndex="46" />
            <textevent timeoffset="80" message="Just a short recovery period before we go again." locIndex="47" />
            <textevent timeoffset="170" message="Into the 2nd effort now." locIndex="48" />
            <textevent timeoffset="190" message="Hit the target cadence." locIndex="49" />
            <textevent timeoffset="220" message="This is the level of intensity required to power over the climbs outdoors." locIndex="50" />
            <textevent timeoffset="300" message="That&apos;s 2 down, 2 to go!" locIndex="51" />
            <textevent timeoffset="370" message="Straight to 80rpm." locIndex="52" />
            <textevent timeoffset="390" message="Stand on the pedals if you have to." locIndex="53" />
            <textevent timeoffset="500" message="Last effort of this block. Make this your best one!" locIndex="54" />
            <textevent timeoffset="550" message="Straight to 80rpm" locIndex="55" />
            <textevent timeoffset="570" message="Big power from you!" locIndex="56" />
            <textevent timeoffset="660" message="Great work! That was a big block of work." locIndex="57" />
        </IntervalsT>
        <SteadyState Duration="180" Power="0.5" pace="1481003871">
            <textevent timeoffset="30" message="Fantastic workout from you." locIndex="58" />
            <textevent timeoffset="60" message="That was some very good power you hit." locIndex="59" />
