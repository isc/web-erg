            <textevent timeoffset="100" message="If you don&apos;t rest properly, you won&apos;t be able to do the efforts properly. Rest is key." locIndex="14" />
        </SteadyState>
        <SteadyState Duration="720" Power="0.94449997" pace="8268" Cadence="60">
            <textevent timeoffset="360" message="Half way" locIndex="15" />
