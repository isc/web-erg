        <SteadyState Duration="15" Power="0.9">
		</SteadyState>
        <SteadyState Duration="15" Power="1">
            <textevent timeoffset="0" message="Up, up, up!" locIndex="6" />
        </SteadyState>
        <SteadyState Duration="15" Power="1.1">
		</SteadyState>
        <SteadyState Duration="30" Power="0.6">
            <textevent timeoffset="0" message="Nice work, easy smooth RPM&apos;s here in recovery. " locIndex="7" />
        </SteadyState>
        <SteadyState Duration="15" Power="0.8">
