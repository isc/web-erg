        <SteadyState Duration="15" Power="0.9">
		</SteadyState>
        <SteadyState Duration="15" Power="1">
            <textevent timeoffset="0" message="Keep the RPM&apos;s up as we get over 100% to make it sting less, but to also let the legs come to life!" locIndex="3" />
        </SteadyState>
        <SteadyState Duration="15" Power="1.1">
		</SteadyState>
        <SteadyState Duration="30" Power="0.6">
