            <textevent timeoffset="150" message="How does 80rpm feel?" locIndex="23" />
        </Ramp>
        <Ramp Duration="240" PowerLow="0.91" PowerHigh="1.04" Cadence="80">
            <textevent timeoffset="10" message="Onto the steeper portion now!" locIndex="24" />
            <textevent timeoffset="30" message="More power required at the same rpm." locIndex="25" />
            <textevent timeoffset="220" message="Now to race to the top!" locIndex="26" />
        </Ramp>
