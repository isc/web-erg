    </tags>
    <workout>
        <SteadyState Duration="300" Power="0.5">
            <textevent timeoffset="254" message="Hydrate for the efforts ahead." locIndex="1" />
        </SteadyState>
        <SteadyState Duration="180" Power="0.7" />
        <SteadyState Duration="120" Power="0.5">
            <textevent timeoffset="110" message="Now we have a 3min threshold effort to prepare for the work ahead." locIndex="2" />
        </SteadyState>
        <SteadyState Duration="180" Power="0.9" />
        <SteadyState Duration="180" Power="0.5" />
        <IntervalsT Repeat="3" OnDuration="30" OffDuration="210" OnPower="0.9" OffPower="0.75">
            <textevent timeoffset="60" message="Aerobic Endurance workouts are aimed at improving your efficiency at lower intensity levels. " locIndex="3" />
            <textevent timeoffset="80" message="Training your muscles to access fat as its main fuel source." locIndex="4" />
            <textevent timeoffset="100" message="Preserving your precious glycogen stores for the finale." locIndex="5" />
        </IntervalsT>
        <SteadyState Duration="180" Power="0.5" />
        <IntervalsT Repeat="3" OnDuration="30" OffDuration="210" OnPower="0.9" OffPower="0.75">
            <textevent timeoffset="60" message="To add an extra stimulus to the session we include a 30sec surge every 2.5min" locIndex="6" />
            <textevent timeoffset="90" message="Your HR should not rise too much during this aerobic effort." locIndex="7" />
            <textevent timeoffset="200" message="Is your cadence remaining the same throughout the effort?" locIndex="8" />
        </IntervalsT>
        <SteadyState Duration="180" Power="0.5">
            <textevent timeoffset="30" message="Today should be a &apos;comfortable&apos; day for you." locIndex="9" />
            <textevent timeoffset="100" message="There are good gains to make with effective lower power sessions." locIndex="10" />
        </SteadyState>
        <IntervalsT Repeat="3" OnDuration="30" OffDuration="210" OnPower="0.9" OffPower="0.75">
