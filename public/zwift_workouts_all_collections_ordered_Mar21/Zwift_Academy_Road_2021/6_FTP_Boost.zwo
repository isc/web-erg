            <textevent timeoffset="60" message="Looking fantastic so far! Create power as efficiently as you can." locIndex="56" />
            <textevent timeoffset="120" message="HALFWAY! Keep pushing deeper into the pain cave." locIndex="57" />
            <textevent timeoffset="180" message="1min to go here! GET THERE!" locIndex="58" />
            <textevent timeoffset="230" message="Picking it up to FTP again in 10sec!" locIndex="59" />
        </SteadyState>
        <SteadyState Duration="180" Power="1">
            <textevent timeoffset="0" message="Put a little more mustard on it!" locIndex="60" />
            <textevent timeoffset="60" message="GREAT WORK! This is what it takes to improve your fitness." locIndex="61" />
            <textevent timeoffset="120" message="Final minute! How bad do you want it?!" locIndex="62" />
            <textevent timeoffset="150" message="POWER!" locIndex="63" />
            <textevent timeoffset="170" message="GET AFTER IT!" locIndex="64" />
        </SteadyState>
        <SteadyState Duration="300" Power="0.55">
            <textevent timeoffset="0" message="Nice one! Let&apos;s take it down easy again and recover before the last set of the day :-)" locIndex="65" />
            <textevent timeoffset="60" message="Take a few deep breaths here...steady as you go." locIndex="66" />
            <textevent timeoffset="120" message="Bring the heart rate down." locIndex="67" />
            <textevent timeoffset="240" message="In 1min, we&apos;re starting our finale! Make this the best effort yet!" locIndex="68" />
            <textevent timeoffset="290" message="Start spinning the legs up again here, CRUSH THIS!" locIndex="69" />
        </SteadyState>
        <SteadyState Duration="30" Power="1.6">
            <textevent timeoffset="0" message="GO GO GO!" locIndex="70" />
            <textevent timeoffset="10" message="PUSH IT!" locIndex="71" />
            <textevent timeoffset="20" message="DON&apos;T LET THE CADENCE DROP!" locIndex="72" />
        </SteadyState>
        <SteadyState Duration="60" Power="0.55">
            <textevent timeoffset="0" message="NICE ONE! Back down again one last time...just 7min of work left for the day!" locIndex="73" />
            <textevent timeoffset="50" message="Ready or not..." locIndex="74" />
        </SteadyState>
        <SteadyState Duration="240" Power="0.95">
            <textevent timeoffset="0" message="You know the drill! Settle in for another solid block of work at threshold!" locIndex="75" />
            <textevent timeoffset="60" message="It&apos;s normal for your legs to feel fatigued by now—KEEP PUSHING, GET STRONGER!" locIndex="76" />
            <textevent timeoffset="120" message="Great work so far! Stay focused and keep things turning over smoothly." locIndex="77" />
            <textevent timeoffset="180" message="1min to go here, then we&apos;re going to lift the intensity right to FTP." locIndex="78" />
            <textevent timeoffset="230" message="Final bit of work for the day coming up!" locIndex="79" />
        </SteadyState>
        <SteadyState Duration="180" Power="1">
            <textevent timeoffset="0" message="C&apos;MON! DRIVE THIS HOME!" locIndex="80" />
            <textevent timeoffset="60" message="Stay focused! Fight for this and push through to the end!" locIndex="81" />
            <textevent timeoffset="120" message="LAST MINUTE!" locIndex="82" />
            <textevent timeoffset="150" message="DIG DIG DIG!" locIndex="83" />
