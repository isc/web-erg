            <textevent timeoffset="270" message="Keep the cadence here! STAY ON THE GAS!" locIndex="94" />
            <textevent timeoffset="290" message="GET TO THE ARCH!" locIndex="95" />
        </SteadyState>
        <Cooldown Duration="300" PowerLow="0.65" PowerHigh="0.45">
            <textevent timeoffset="0" message="Phew! Really nice effort there, my friend!" locIndex="96" />
            <textevent timeoffset="10" message="You showed that you&apos;re in this to win it, and have the mental fortitude to see it through." locIndex="97" />
            <textevent timeoffset="20" message="We&apos;ll take 5min to cool down now, then check this one off the list." locIndex="98" />
            <textevent timeoffset="120" message="Keep easing off the gas here." locIndex="99" />
            <textevent timeoffset="240" message="Thank you for showing up, working hard, and putting the effort in today." locIndex="100" />
            <textevent timeoffset="250" message="Your FTP will thank you :-)" locIndex="101" />
            <textevent timeoffset="260" message="Be sure to refuel and rehydrate well between sessions." locIndex="102" />
            <textevent timeoffset="270" message="And good luck with your remaining Zwift Academy workouts and group rides." locIndex="103" />
            <textevent timeoffset="280" message="You&apos;re one step closer to the Finish Line!" locIndex="104" />
            <textevent timeoffset="290" message="Ride On!" locIndex="105" />
        </Cooldown>
    </workout>
