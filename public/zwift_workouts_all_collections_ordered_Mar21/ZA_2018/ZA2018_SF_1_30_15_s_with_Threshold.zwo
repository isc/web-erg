        </Ramp>
        <IntervalsT Repeat="10" OnDuration="30.000002" OffDuration="15.000001" OnPower="1.2544999" OffPower="0.65449995" pace="-1307163960">
            <textevent timeoffset="35" message="How was that intensity?" locIndex="43" />
