            <textevent timeoffset="90" message="Again, good recovery before repeating the effort." locIndex="37" />
        </SteadyState>
        <SteadyState Duration="240" Power="1.17" pace="0">
            <textevent timeoffset="30" message="Aim to stick to the target power." locIndex="38" />
            <textevent timeoffset="90" message="Keep your breathing controlled." locIndex="39" />
            <textevent timeoffset="150" message="Focus on your cadence." locIndex="40" />
            <textevent timeoffset="210" message="Only 30s left. Push right to the end!" locIndex="41" />
        </SteadyState>
        <SteadyState Duration="300" Power="0.65074277" pace="0">
            <textevent timeoffset="30" message="Well done." locIndex="42" />
            <textevent timeoffset="90" message="That was hard work. But you completed it!" locIndex="43" />
            <textevent timeoffset="150" message="Full recovery again before we begin the final set." locIndex="44" />
            <textevent timeoffset="210" message="Next is 4 x 1min efforts with short recovery." locIndex="45" />
            <textevent timeoffset="280" message="Here we go." locIndex="46" />
        </SteadyState>
        <IntervalsT Repeat="4" OnDuration="60" OffDuration="60" OnPower="1.2045" OffPower="0.65449995" pace="0">
            <textevent timeoffset="20" message="High power for 1min." locIndex="47" />
            <textevent timeoffset="40" message="Maintain the same cadence." locIndex="48" />
            <textevent timeoffset="80" message="Just a short recovery period." locIndex="49" />
            <textevent timeoffset="110" message="Ready to go again." locIndex="50" />
            <textevent timeoffset="150" message="Half way. Focus on controlled breathing." locIndex="51" />
            <textevent timeoffset="230" message="Just 2 efforts to go." locIndex="52" />
            <textevent timeoffset="310" message="Compose yourself during the recovery period." locIndex="53" />
            <textevent timeoffset="350" message="Into the last effort now." locIndex="54" />
            <textevent timeoffset="390" message="Finish the set strong!" locIndex="55" />
        </IntervalsT>
        <SteadyState Duration="300" Power="0.50449997" pace="0">
            <textevent timeoffset="60" message="Well done. That was a difficult session today." locIndex="56" />
            <textevent timeoffset="120" message="All quality, hard intervals" locIndex="57" />
            <textevent timeoffset="180" message="It&apos;s important to spin the legs out to recover for your next session." locIndex="58" />
        </SteadyState>
