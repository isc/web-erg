        <SteadyState Duration="60.000004" Power="0.50449997" pace="-1307163960" />
        <SteadyState Duration="60.000004" Power="0.95449996" pace="-1307163960">
            <textevent timeoffset="10" message="Power is at threshold, but because the duration is short, this one won&apos;t trouble you." locIndex="4" />
        </SteadyState>
