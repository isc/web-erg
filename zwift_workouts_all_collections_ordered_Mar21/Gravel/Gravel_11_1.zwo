        <SteadyState Duration="60" Power="0.75">
            <textevent timeoffset="0" message="Not hard at all, but worth the effort. " locIndex="3" />
        </SteadyState>
        <SteadyState Duration="60" Power="0.85">
            <textevent timeoffset="0" message="We&apos;ll need all the warming up we can get today. " locIndex="4" />
