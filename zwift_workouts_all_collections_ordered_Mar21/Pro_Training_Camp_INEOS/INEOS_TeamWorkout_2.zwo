        <Cooldown Duration="300" PowerLow=".5" PowerHigh=".65">
            <textevent timeoffset="0" message="Great session." locIndex="93" />
            <textevent timeoffset="10" message="You&apos;ve made some serious gains today." locIndex="94" />
            <textevent timeoffset="20" message="This is a great session to repeat if you want to hit all the zones." locIndex="95" />
            <textevent timeoffset="30" message="Take the next few minutes to cool down." locIndex="96" />
