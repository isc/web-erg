            <textevent timeoffset="300" message="Get after it!" locIndex="60" />
            <textevent timeoffset="330" message="Don&apos;t let up now." locIndex="61" />
            <textevent timeoffset="360" message="Stay with it. Last effort up next." locIndex="62" />
