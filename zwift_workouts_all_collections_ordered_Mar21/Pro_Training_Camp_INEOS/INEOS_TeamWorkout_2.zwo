        </SteadyState>
        <IntervalsT Repeat="4" OnDuration="60" OffDuration="60" OnPower=".70" OffPower="1.15">
            <textevent timeoffset="0" message="Starting in zone 2" locIndex="50" />
            <textevent timeoffset="10" message="Be conservative here, the harder efforts are coming." locIndex="51" />
            <textevent timeoffset="60" message="Let&apos;s see some power!" locIndex="52" />
            <textevent timeoffset="90" message="Keep it up." locIndex="53" />
