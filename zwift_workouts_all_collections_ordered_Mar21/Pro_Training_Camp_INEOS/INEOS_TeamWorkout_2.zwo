    <tags>
        <tag name="zprotraining" />
    </tags>
    <workout>
        <Warmup Duration="300" PowerLow=".55" PowerHigh=".65">
            <textevent timeoffset="0" message="Welcome to Zwift&apos;s Pro Training Camp." locIndex="1" />
            <textevent timeoffset="10" message="Let&apos;s get started with an easy warm-up. Keep your cadence comfortable." locIndex="2" />
            <textevent timeoffset="20" message="In this training series we feature workouts from the world&apos;s best cycling teams." locIndex="3" />
            <textevent timeoffset="30" message="Giving YOU the chance to train like a pro." locIndex="4" />
            <textevent timeoffset="40" message="Today&apos;s workout is from INEOS Grenadiers and takes you through all the paces." locIndex="5" />
            <textevent timeoffset="60" message="Typically there are 7 training zones." locIndex="6" />
            <textevent timeoffset="70" message="Zones 1 is active recovery, zone 2 is endurance, zone 3 is Tempo... " locIndex="7" />
            <textevent timeoffset="80" message="Zone 4 is Threshold, zone 5 is VO2max, zone 6 is anaerobic capacity..." locIndex="8" />
            <textevent timeoffset="90" message="And zone 7 is neuromuscular power." locIndex="9" />
            <textevent timeoffset="100" message="For today that means touching all 7." locIndex="10" />
            <textevent timeoffset="110" message="We&apos;ll get started with 10 min at zone 2, right into 5 x 30 seconds on, 30 seconds off." locIndex="11" />
            <textevent timeoffset="120" message="The &apos;On&apos; efforts will progress from zone 3 to zone 5." locIndex="12" />
            <textevent timeoffset="130" message="Then we will finish with 2 x 8 min blocks." locIndex="13" />
            <textevent timeoffset="140" message="The 1st block will be 4 x 60 seconds at zone 2, 60 seconds at zone 5." locIndex="14" />
            <textevent timeoffset="150" message="The 2nd block will be 8 x 20 seconds at zone 6, 40 seconds zone 1." locIndex="15" />
            <textevent timeoffset="170" message="Take the next few minutes to finish warming up." locIndex="16" />
            <textevent timeoffset="290" message="Let&apos;s get this session started." locIndex="17" />
        </Warmup>
        <SteadyState Duration="300" Power=".75">
            <textevent timeoffset="0" message="This first 10 min block is at the top of zone 2." locIndex="18" />
            <textevent timeoffset="30" message="Settle in and find a comfortable cadence." locIndex="19" />
            <textevent timeoffset="60" message="Stay on top of your hydration, especially early in this workout." locIndex="20" />
            <textevent timeoffset="180" message="Nice work." locIndex="21" />
            <textevent timeoffset="190" message="Keep it steady." locIndex="22" />
            <textevent timeoffset="240" message="Looking great so far." locIndex="23" />
            <textevent timeoffset="260" message="We are going right into 5 x 30 seconds on, 30 seconds off." locIndex="24" />
            <textevent timeoffset="270" message="The &apos;on&apos; efforts will progress from zone 3 to zone 5." locIndex="25" />
            <textevent timeoffset="280" message="Feeling ready? We&apos;re going either way!" locIndex="26" />
        </SteadyState>
        <SteadyState Duration="30" Power=".82">
            <textevent timeoffset="0" message="Small increase in effort." locIndex="27" />
        </SteadyState>
        <SteadyState Duration="30" Power=".55">
            <textevent timeoffset="0" message="Nice and easy." locIndex="28" />
            <textevent timeoffset="10" message="Increase in effort up next." locIndex="29" />
        </SteadyState>
        <SteadyState Duration="30" Power="1">
            <textevent timeoffset="0" message="Bring up the effort." locIndex="30" />
            <textevent timeoffset="10" message="Settle into it." locIndex="31" />
        </SteadyState>
        <SteadyState Duration="30" Power=".55">
            <textevent timeoffset="" message="Great work." locIndex="32" />
            <textevent timeoffset="" message="We are going up in effort again." locIndex="33" />
        </SteadyState>
        <SteadyState Duration="30" Power="1.1">
            <textevent timeoffset="0" message="Let&apos;s go!" locIndex="34" />
            <textevent timeoffset="10" message="Keep the pressure on." locIndex="35" />
        </SteadyState>
        <SteadyState Duration="30" Power=".55">
            <textevent timeoffset="0" message="Easy now." locIndex="36" />
        </SteadyState>
        <SteadyState Duration="30" Power="1.15">
            <textevent timeoffset="0" message="Step it up!" locIndex="37" />
            <textevent timeoffset="10" message="Keep fighting." locIndex="38" />
        </SteadyState>
        <SteadyState Duration="30" Power=".55">
            <textevent timeoffset="0" message="Nice work." locIndex="39" />
            <textevent timeoffset="10" message="One effort left." locIndex="40" />
        </SteadyState>
        <SteadyState Duration="30" Power="1.3">
            <textevent timeoffset="0" message="Go, go, go!" locIndex="41" />
            <textevent timeoffset="10" message="Get across that line!" locIndex="42" />
        </SteadyState>
        <SteadyState Duration="300" Power=".55">
            <textevent timeoffset="0" message="Nice work." locIndex="43" />
            <textevent timeoffset="10" message="Take the next 5 minutes to fully recover." locIndex="44" />
            <textevent timeoffset="20" message="We&apos;ve got more work today." locIndex="45" />
            <textevent timeoffset="30" message="Next block is 60 seconds at zone 2, 60 seconds at zone 5." locIndex="46" />
