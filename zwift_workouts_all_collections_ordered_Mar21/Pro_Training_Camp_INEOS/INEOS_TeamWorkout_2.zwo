            <textevent timeoffset="20" message="Nice work. These are going to come at you fast." locIndex="73" />
            <textevent timeoffset="30" message="Recover as much as possible." locIndex="74" />
            <textevent timeoffset="60" message="Up, up, up!" locIndex="75" />
            <textevent timeoffset="80" message="Ease off." locIndex="76" />
            <textevent timeoffset="120" message="Let&apos;s go!" locIndex="77" />
            <textevent timeoffset="140" message="3 down, 5 to go." locIndex="78" />
            <textevent timeoffset="150" message="Quick drink of water." locIndex="79" />
            <textevent timeoffset="180" message="Back on it!" locIndex="80" />
            <textevent timeoffset="200" message="Recover." locIndex="81" />
            <textevent timeoffset="210" message="Halfway there." locIndex="82" />
            <textevent timeoffset="240" message="Go, go, go!" locIndex="83" />
            <textevent timeoffset="260" message="Nice work, 5 down, 3 to go." locIndex="84" />
            <textevent timeoffset="300" message="Power to the pedals." locIndex="85" />
            <textevent timeoffset="320" message="You are crushing this session." locIndex="86" />
            <textevent timeoffset="330" message="Keep up the good work." locIndex="87" />
            <textevent timeoffset="360" message="Let&apos;s go! Stay strong." locIndex="88" />
            <textevent timeoffset="380" message="Back off the power quickly." locIndex="89" />
            <textevent timeoffset="390" message="Spin out those legs." locIndex="90" />
            <textevent timeoffset="420" message="Last one! You can do it." locIndex="91" />
