            <textevent timeoffset="20" message="I need you feeling fresh for the last round." locIndex="67" />
            <textevent timeoffset="260" message="The final block of work is 8 x 20 seconds in zone 6, 40 seconds in zone 1." locIndex="68" />
            <textevent timeoffset="270" message="Feeling up for it?" locIndex="69" />
            <textevent timeoffset="280" message="I know you are." locIndex="70" />
            <textevent timeoffset="290" message="Get ready." locIndex="71" />
        </SteadyState>
        <IntervalsT Repeat="8" OnDuration="20" OffDuration="40" OnPower="1.4" OffPower=".55">
