            <textevent timeoffset="120" message="Same again now. Power over the hill." locIndex="65" />
            <textevent timeoffset="140" message="Into the last interval for the day!" locIndex="66" />
            <textevent timeoffset="160" message="make it the best one!" locIndex="67" />
        </SteadyState>
        <Ramp Duration="180" PowerLow="0.75" PowerHigh="0.95" Cadence="65">
            <textevent timeoffset="10" message="Hit that target rpm of 65." locIndex="68" />
            <textevent timeoffset="30" message="Position yourself well on the bike." locIndex="69" />
            <textevent timeoffset="50" message="On the back of the seat with hands relaxed on the bars." locIndex="70" />
            <textevent timeoffset="100" message="Picture yourself grinding over a steep dirt hill." locIndex="71" />
        </Ramp>
        <SteadyState Duration="300" Power="0.55">
            <textevent timeoffset="30" message="Great work today." locIndex="72" />
            <textevent timeoffset="60" message="That&apos;s quality work you have completed." locIndex="73" />
            <textevent timeoffset="80" message="You could experience some muscle soreness in the coming days." locIndex="74" />
            <textevent timeoffset="100" message="This is normal and to be expected." locIndex="75" />
            <textevent timeoffset="200" message="Be sure to hydrate and start refueling for your next session." locIndex="76" />
        </SteadyState>
    </workout>
